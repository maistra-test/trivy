#!/bin/sh
set -x

cd "$1"

ls

PATCH_GO_VERSION=$(egrep '^go ' go.mod | awk '{print $2}')
PATCH_GO_PATH="${RUNNER_TOOL_CACHE}/go/${PATCH_GO_VERSION}.*/*/bin"
export PATH="${PATH}:${PATCH_GO_PATH}"

echo "Environment variables are"
env | sed -e 's+^+    +'

echo "Executing go version and status"
go version
go status
